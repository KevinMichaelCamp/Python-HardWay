    "import numpy as np\n",
    "\n",
    "p1 = np.array([1,1])\n",
    "p2 = np.array([4,4])\n",
    "\n",
    "def distance(p1, p2):\n",
    "    d = np.sqrt(np.sum(np.power(p2 - p1, 2)))\n",
    "    return d\n",
    "\n",
    "distance(p1, p2)"
